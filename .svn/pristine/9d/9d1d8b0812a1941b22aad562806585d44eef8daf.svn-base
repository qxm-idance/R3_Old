import * as types from './mutation-types';

export const updateBreadcrumbs = ({ dispatch, state }, breadcrumbs) => {
  dispatch(types.UPDATE_BREADCRUMBS, breadcrumbs);
};

export const updateBrand = ({ dispatch, state }, event) => {
  dispatch(types.UPDATE_BRAND, event.target.value);
};
export const updateModel = ({ dispatch, state }, event) => {
  dispatch(types.UPDATE_MODEL, event.target.value);
};
export const updateColor = ({ dispatch, state }, event) => {
  dispatch(types.UPDATE_COLOR, event.target.value);
};
