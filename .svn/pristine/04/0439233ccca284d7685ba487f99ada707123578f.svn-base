 .radios--color label:before {
    content: "";
    display: block;
    border-radius: 50%;
    width: 13px;
    height: 13px;
    position: static;
    padding: 0;
    line-height: 13px;
    -webkit-transition: font-size .2s ease-out;
    transition: font-size .2s ease-out;
    margin-top: 3px;
    margin-bottom: 3px;
    margin-left: auto;
    margin-right: auto;
    box-shadow: inset 1px 1px 3px rgba(0,0,0,0.3);
}
