    text-align: center;
}
/**/
.article img {
    height: auto;
    max-width: 100%;
}
.article--expand-right img {
    position: absolute;
    top: 0;
    left: 0;
    height: auto;
    width: auto;
    max-width: none;
    margin: 0;
}
