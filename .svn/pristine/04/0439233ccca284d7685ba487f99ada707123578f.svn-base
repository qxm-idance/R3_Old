    /*width: 40px;*/
    background: -webkit-linear-gradient(left, rgba(255,255,255,0) 0%, #fff 100%);
    background: linear-gradient(to right, rgba(255,255,255,0) 0%, #fff 100%);
}
.product-grid__item .product-price {
    display: block;
    font-family: "telenor", sans-serif;
