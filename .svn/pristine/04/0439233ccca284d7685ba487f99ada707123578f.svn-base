    margin-top: 0.313rem;
    line-height: 1.1;
}
.product-grid__item .category-link {
    font-family: "telenor", sans-serif;
    font-weight: normal;
    display: block;
    color: #757575;
    position: absolute;
    top: 70px;
    padding-left: 0.8rem;
    margin-top: 0.313rem;
}
.product-grid__item .category-link:before {
    font-size: 0.438rem;
    position: absolute;
    top: 0;
    left: 0;
}
.icon-arrow-next:before {
    content: "\E00B";
}
.product-grid__item .category-link {
    font-size: 0.813rem;
    line-height: .9375rem;
}

/*Product grid item overlay*/
