    bottom: auto;
    left: auto;
}
.list-item--large .list-item__status {
    margin-bottom: 0.875rem;
}
.list-item--large [class*="button"] {
    margin: 0 0 1.25rem 0;
    position: relative;
    left: auto;
    bottom: auto;
}
@media only screen and (min-width: 37.5em){
    .list-item--large [class*="button"] {
        margin: 0;
    }
}
.list-item--large .list-item__note {
    position: absolute;
    left: auto;
    right: 0;
    bottom: 0;
    width: 50%;
    padding-left: 0;
    margin: 0;
    padding-top: 0;
    background: #fff;
}

/*List item splashes*/
.list-item__splash--yellow {
    border-color: #ffb900;
    color: #ffb900;
}
.list-item__splash--red {
    border-color: #db5451;
    color: #db5451;
}
.list-item__splash--purple {
    border-color: #6b2377;
    color: #6b2377;
}
.list-item__splash--small .text--small, .list-item__splash--small .text--content code, .text--content .list-item__splash--small code {
    font-size: 1.5rem;
}
/*Rating stars*/
.rating__stars+.rating__text {
    display: block;
    font-family: "telenor", sans-serif;
    font-weight: normal;
    font-size: 1rem;
    line-height: 1;
}
.rating__stars--small+.rating__text {
    font-family: "telenor", sans-serif;
    font-weight: normal;
    font-size: 0.813rem;
    line-height: .9375rem;
}
.box--expand-above-border {
    margin: -1px;
}
@media only screen and (min-width: 37.5em){
    .box--blue, .title--section, .box--yellow, .box--yellow-light, .box--purple, .box--green {
        padding: 1.25rem 1.25rem 1.25rem 1.25rem !important;
    }
}

