}
.icon-bullet:before, .bullet-link:before, .button--link:before, .bullet-link--reverse:before {
    content: "\E01F";
}
.list-item--hero .list-item__note {
    display: block;
    position: static;
    background-color: transparent;
    border-top: 1px solid #d1d1d1;
    padding: 0.625rem 0 0;
    font-size: 0.563rem;
    line-height: 1.3;
    color: #757575;
}


/*hreo full width.css*/
.list-item--hero--full-width {
    height: 350px !important;
    background-image: none;
}
