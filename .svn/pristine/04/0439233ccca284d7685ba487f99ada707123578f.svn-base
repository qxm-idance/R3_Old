    margin-bottom: 0.625rem;
}
.list-item [class*="list--check"] {
    margin: 0 0 1.25rem;
}
.clear:after, .terms-list dd:after, .media:after, .container:after, .section:after, .block:after, .box--default:after, .box--default-dark-border:after, .block--content:after, .block--content--dark:after, .block--col:after, .block__header:after, .block__content:after, .button:after, .form-checkbox--filter label:after, .button-row--small:after, .button-row--med:after, .button--tabs:after, .button--sg-tabs:after, .button--steps:after, .button--steps a:after, .product-list:after, .list-item:after, .list-item__inner-wrapper:after,
.list-item__note:after, .list-item__subscription:after, .list-item .product-name:after, .list-item .list-item--hero__call-to-action:after, .product-grid:after, .product-grid__item__inner-wrapper:after, .radios--color:after, .table--order p:after, .pager--default:after, .paging_simple_numbers:after, .tabpanel__list:after, .sg-color:after, .sg-icons-list:after {
