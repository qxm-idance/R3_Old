}


/*Article section*/
.section--padding {
    padding-top: 3.125rem;
    padding-bottom: 3.125rem;
}
.block__header {
    padding-bottom: 0.938rem;
    border-bottom: 1px solid #d1d1d1;
    position: relative;
}
.leader--large {
