<button class="button button--default" data-component="collapse" data-alias="collapseSimpleClose">
	<span class="button__text">
		Collapse
	</span>
</button>
<div class="collapse__target collapse--closed" data-element="collapseSimpleClose:collapseTarget">
	<div data-element="collapseSimpleClose:collapseContent">
		Collapsible block content
		<button class="button button--default" data-element="collapseSimpleClose:toggleTrigger">
			<span class="button__text">
				Close
			</span>
		</button>
	</div>
</div>
